title = {{Investigation of the Kinetics of the Back Reaction of Electrons with Tri-Iodide in Dye-Sensitized Nanocrystalline Photovoltaic Cells}},
url = {http://pubs.acs.org/doi/abs/10.1021/jp001185z},
volume = {104},
year = {2000}
}
