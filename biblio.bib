@article{Barnes2011,
abstract = {A simple and powerful approach for assessing the recombination losses in dye-sensitized solar cells across the current-voltage curve as a function of TiO2 electron concn. is demonstrated. The total flux of electrons recombining with iodine species in the electrolyte and oxidized dye mols. can be thought of as a recombination c.d., defined as jrec = jinj-j where jinj is the current of electrons injected from optically excited dye states and j is the c.d. collected at cell voltage. The electron concn. at any given operating condition is detd. by charge extn. This allows comparison of factors influencing electron recombination rates at matched electron concn. We show that jrec is typically 2-3 times higher under 1 sun equiv. illumination (jinj {\textgreater} 0) relative to dark (jinj = 0) conditions. This difference was increased by increasing light intensity, electrolyte iodine concn. and electrolyte solvent viscosity. The difference was reduced by increasing the electrolyte iodide concn. and increasing the temp. These results allowed us to verify a numerical model of complete operational cells (Barnes et al., Phys. Chem. Chem. Phys., DOI: 10.1039/c0cp01554g) and to relate the differences in jrec to phys. processes in the devices. The difference between jrec in the light and dark can be explained by two factors: (a) an increase in the concn. of electron acceptor species (I3- and/or I2) when current is flowing under illumination relative to dark conditions where the current is flowing in the opposite direction, and (b) a non-trivial contribution from electron recombination to oxidized dye mols. under light conditions. More generally, the technique helps to assign the obsd. relationship between the components, processing and performance of dye-sensitized solar cells to more fundamental phys. processes. [on SciFinder(R)]},
annote = {CAPLUS AN 2011:158598(Journal)},
author = {Barnes, Piers R F and Anderson, Assaf Y and Juozapavicius, Mindaugas and Liu, Lingxuan and Li, Xiaoe and Palomares, Emilio and Forneli, Amparo and O'Regan, Brian C},
doi = {10.1039/C0CP01855D},
issn = {1463-9076},
journal = {Phys. Chem. Chem. Phys.},
keywords = {dye sensitized solar cell charge recombination dar,ig},
mendeley-tags = {ig},
number = {8},
pages = {3547--3558},
publisher = {Royal Society of Chemistry},
title = {{Factors controlling charge recombination under dark and light conditions in dye sensitised solar cells}},
url = {http://xlink.rsc.org/?DOI=C0CP01855D},
volume = {13},
year = {2011}
}
