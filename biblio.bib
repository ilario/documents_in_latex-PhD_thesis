journal = {National Renewable Energy Laboratory},
keywords = {GIS,January 2016,LiDAR,NREL/TP-6A20-65298,PV,Photovoltaic,Rooftop,Technical Potential,city,energy generation,ig,installed capacity,large buildings,medium buildings,nation,small buildings,state},
mendeley-tags = {ig},
number = {January},
title = {{Rooftop Solar Photovoltaic Technical Potential in the United States: A Detailed Assessment}},
url = {https://www.nrel.gov/docs/fy16osti/65298.pdf},
year = {2016}
}
